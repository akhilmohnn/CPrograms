#include<stdio.h>
void main()
{
char c;
printf("enter thecharacter:")
scanf("%c",&c);
	switch(c)
	{
	case 'A'...'Z': printf("You entered alphabet\n");
	break;
	case 'a'...'z': printf("You entered alphabet");
	break;
	case '0'...'9': printf("You entered a digit");
	break;
	default:printf("special character");
	}
}
	 
